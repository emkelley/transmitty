    "adm-zip": "^0.5.9",
    "colors": "^1.4.0",
    "figlet": "^1.5.2",
    "node-notifier": "^10.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@types/rimraf": "^3.0.2",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
